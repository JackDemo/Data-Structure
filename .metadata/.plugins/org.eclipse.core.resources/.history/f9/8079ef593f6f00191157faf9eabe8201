package main;

public class Test {

	public static void main(String[] args) {
		
		int[] nums = new int[] {1,2,3,4};
		int[] numsTwo = nums;
		
		for (int j = 0 ; j < 3 ; j++) {
			System.out.print(j);
			numsTwo[j+1] = nums[j];
		}	
		System.out.println(nums.toString());
		System.out.println(numsTwo.toString());
//		System.out.println(this.toString());
		for (int j = 3-1 ; j>=0 ; j--) {
			numsTwo[j+1] = nums[j];
		}
		System.out.println(nums.toString());
		System.out.println(numsTwo.toString());

	}

}
