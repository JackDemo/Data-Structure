package main;

public class Test {

	public static void main(String[] args) {
		for (int j = 0 ; j < 4 ; j++) {
			System.out.print(j);
//			this.element[j+1] = source[j];
		}	
		System.out.println();
//		System.out.println(this.toString());
		for (int j = 4-0 ; j>=0 ; j--) {
			System.out.print(j);
		}

	}

}
